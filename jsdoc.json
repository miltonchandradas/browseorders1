{
	"source": {
		"include": ["webapp"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs|test|localService)"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"recurse": true,
		"destination": "./dist/docs/"
	}
}